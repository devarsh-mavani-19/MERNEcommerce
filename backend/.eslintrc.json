{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"plugins": ["node"],
	"globals": {
		"process": "readonly"
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"]
	}
}
